{
  "name": "nzta/silverstripe-okta-api",
  "description": "Integration with the Okta API",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/NZTA/silverstripe-okta-api",
  "keywords": [
    "silverstripe",
    "okta",
    "cms"
  ],
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/NZTA/silverstripe-okta-api/issues"
  },
  "require": {
    "silverstripe/framework": "^4.2",
    "silverstripe/cms": "^4.2",
    "symbiote/silverstripe-queuedjobs": "^4.2"
  },
  "require-dev": {
    "phake/phake": "^3.1",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3"
  },
  "scripts": {
    "lint": "phpcs -s src/ tests/",
    "lint-fix": "phpcbf -s src/ tests/",
    "test": "phpunit -d memory_limit=512M -d flush=1"
  },
  "extra": [],
  "autoload": {
    "psr-4": {
      "NZTA\\OktaAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NZTA\\OktaAPI\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
